    "prettier-plugin-package": "^1.3.0",
    "turbo": "latest",
    "typescript": "^4.5.3"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.32.3"
